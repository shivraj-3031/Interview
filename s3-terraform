############### provider.tf ################
provider "aws" {
  region = "us-east-1"
}

############## main.tf #################
resource "aws_s3_bucket" "my_bucket" {
  bucket = "devops-demo-bucket"
  acl = "private"

  tags = {
    Name = "devops-demo-bucket"
    Environment = "dev"
  }
}
           ### enable versioning###
resource "aws_s3_bucket_versioning" "versioning" {
  bucket = aws_s3_bucket.my_bucket.id

  versioning_configuration {
    status = "Enabled"
  }
}  
           
